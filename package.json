{
  "name": "we-plugin-widget",
  "description": "We.js we-plugin-widget plugin",
  "version": "2.0.0",
  "main": "plugin.js",
  "scripts": {
    "test": "BLUEBIRD_DEBUG=true NODE_ENV=test LOG_LV=info ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js",
    "coverage": "NODE_ENV=test LOG_LV=info ./node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports -R spec test/bootstrap.js test/**/*.test.js"
  },
  "homepage": "https://github.com/wejs/we-plugin-widget",
  "author": "Alberto Souza <contato@albertosouza.net>",
  "files": [
    "plugin.js",
    "install.js",
    "lib",
    "server",
    "files/public"
  ],
  "devDependencies": {
    "express-mysql-session": "^1.2.3",
    "fs-extra": "^5.0.0",
    "istanbul": "^0.4.5",
    "jshint": "^2.9.5",
    "mocha": "^5.0.1",
    "mysql2": "^1.5.2",
    "sinon": "^4.4.1",
    "supertest": "^3.0.0",
    "we-core": "^2",
    "we-plugin-acl": "^1.2.11",
    "we-plugin-auth": "^2",
    "we-plugin-user": "^2",
    "we-plugin-view": "^1.4.3",
    "we-test-tools": "wejs/we-test-tools",
    "we-theme-site-wejs": "wejs/we-theme-site-wejs"
  },
  "repository": "wejs/we-plugin-widget",
  "license": "MIT",
  "keywords": [
    "wejs-plugin"
  ],
  "wejs": {
    "plugins": {
      "we-plugin-acl": true,
      "we-plugin-auth": true,
      "we-plugin-user": true,
      "we-plugin-view": true
    },
    "dependencies": {
      "we-plugin-user": true,
      "we-plugin-view": true
    },
    "devPlugins": {}
  }
